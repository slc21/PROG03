<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:foregroundTint="#C35024"
    tools:context=".RunningActivity">

    <!-- TODO: Update blank fragment layout -->

    <Button
        android:id="@+id/record"
        android:layout_width="106dp"
        android:layout_height="56dp"
        android:backgroundTint="#C35024"
        android:text="Record"
        tools:layout_editor_absoluteX="289dp"
        tools:layout_editor_absoluteY="67dp" />

    <TextView
        android:id="@+id/runningEntry"
        android:layout_width="135dp"
        android:layout_height="51dp"
        android:text="Running"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="67dp" />

    <TextView
        android:id="@+id/dateEntry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Time Elapsed"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="224dp"
        tools:layout_editor_absoluteY="128dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Distance Traveled"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="224dp"
        tools:layout_editor_absoluteY="177dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="200"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="199dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2.4mi"
        tools:layout_editor_absoluteX="224dp"
        tools:layout_editor_absoluteY="199dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Calories Burned"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="177dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Steps Taken"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="128dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1325"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="150dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="40:25"
        tools:layout_editor_absoluteX="224dp"
        tools:layout_editor_absoluteY="150dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="414dp"
        android:layout_height="64dp"
        android:backgroundTint="#C35024"
        android:text="End Run"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="678dp" />

    <com.google.android.gms.maps.MapView
        android:id="@+id/mapView2"
        android:layout_width="388dp"
        android:layout_height="450dp"
        tools:ignore="SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="223dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="417dp"
        android:layout_height="71dp"
        android:backgroundTint="#C35024"
        android:text="Exercise Tracker"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="-8dp" />

</androidx.constraintlayout.widget.ConstraintLayout>